<mvc:View
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns:u="sap.ui.unified"
	displayBlock="true"
	controllerName="com.ey.hcp.eis.controller.Main">

	<App>
		<pages>
			<Page>
				<content>
					<f:SimpleForm>
						<Label text="Document Name: "/>
						<Input id="inpDocName" />
						<Label text="Document Uploaded By: "/>
						<Input id="inpDocUploadedBy"/>
						<Label text="" />
						<Button text="Submit" icon="sap-icon://accept" press="btnSubmitPressed" />
					</f:SimpleForm>
				</content>
			</Page>
		</pages>
	</App>

</mvc:View>